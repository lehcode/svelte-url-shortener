{
	"extends": "./tsconfig.base.json",
	"compilerOptions": {
		"outDir": "./dist/prod",
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"strictBindCallApply": true,
		"strictPropertyInitialization": true,
		"noImplicitThis": true,
		"alwaysStrict": true,
		"checkJs": true,
		"sourceMap": false,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"declaration": true,
		"removeComments": true
	}
}
