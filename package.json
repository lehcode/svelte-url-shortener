{
  "name": "svelte-url-shortener",
  "version": "0.0.1",
  "scripts": {
    "dev": "npx wrangler dev src/index.ts &> /dev/null && vite --mode=preview dev",
    "build": "vite build",
    "preview": "npm run build && wrangler pages dev",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.prod.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.prod.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "deploy": "npm run build && wrangler pages deploy",
    "deploy:worker": "wrangler deploy src/lib/cloudflare/worker.ts --env=preview --outdir=.wrangler/dist/ --tsconfig=tsconfig.dev.json --minify=false",
    "cf-typegen": "wrangler types && mv worker-configuration.d.ts src/"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.2",
    "@cloudflare/workers-types": "^4.20240512.0",
    "@picocss/pico": "^2.0.6",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-cloudflare": "^4.4.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "^8.56.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "esbuild": "^0.21.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "rollup": "^2.0.0",
    "sass": "^1.77.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-preprocess": "^5.1.4",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "uuid": "^9.0.1",
    "vite": "^5.0.3",
    "vitest": "^1.6.0",
    "wrangler": "^3.57.1"
  },
  "type": "module",
  "engines": {
    "node": ">=18.20.0"
  }
}